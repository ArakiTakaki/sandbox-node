{
  "compilerOptions": {
   "target": "ESNEXT",
   "module": "commonjs",
   "sourceMap": true,
   "outDir": "./dist",
   "strict": true,
   "noImplicitAny": true,
   "strictNullChecks": true,
   "strictFunctionTypes": true,
   "strictBindCallApply": true,
   "strictPropertyInitialization": true,
   "noImplicitThis": true,
   "alwaysStrict": true,
   "esModuleInterop": true,
 },
 "include": [
   "src/**/*"
 ],
}
